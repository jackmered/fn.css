{
  "name": "fn.css",
  "version": "3.1.0",
  "main": "dist/fn.css",
  "style": "dist/fn.css",
  "license": "MIT",
  "repository": "jackmered/fn.css",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "css-mqpacker": "^7.0.0",
    "csso": "^4.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.36",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-scss": "^3.19.0"
  },
  "dependencies": {},
  "scripts": {
    "start": "npm-run-all lint clean build minify",
    "build": "npm-run-all scss postcss",
    "scss": "sass scss/fn.scss dist/fn.css",
    "postcss": "postcss --replace \"dist/*.css\"",
    "clean": "rimraf dist",
    "lint": "stylelint scss/*.scss",
    "minify": "csso dist/fn.css -o dist/fn.min.css -s file"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "trailingComma": "all"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "css-mqpacker": {}
    }
  },
  "stylelint": {
    "extends": "stylelint-config-prettier",
    "plugins": [
      "stylelint-scss"
    ]
  }
}
